(Define-Ontology MUTAGENESIS-ONTOLOGY  (creator "Santiago Ontañón")  (description "Ontology for the mutagenesis dataset, it supports the B1, B2, B3 and B4 versions"))(Define-sort (MUTAGENESIS-ONTOLOGY mutagenesis-problem)	(problem ! compound)	(solution ! mutagenesis-solution))(define-sort (MUTAGENESIS-ONTOLOGY mutagenesis-solution))(Define-sort (MUTAGENESIS-ONTOLOGY compound)	(components compound)	(bonds bond)	(lumo ! float)	(logp ! float))(define-sort (compound atom)	(type ! integer)	(charge ! float))(define-sort (atom atom-c))(define-sort (atom atom-h))(define-sort (atom atom-o))(define-sort (atom atom-n))(define-sort (atom atom-f))(define-sort (atom atom-s))(define-sort (atom atom-cl))(define-sort (atom atom-br))(define-sort (atom atom-s))(define-sort (atom atom-i))(define-sort (compound compound-benzene))(define-sort (compound compound-methyl))(define-sort (compound compound-nitro))(define-sort (compound compound-hetero_aromatic_5_ring))(define-sort (compound compound-hetero_aromatic_6_ring))(define-sort (compound compound-carbon_5_aromatic_ring))(define-sort (compound compound-carbon_6_ring))(define-sort (compound compound-ball3))(define-sort (compound compound-anthracene))(define-sort (compound compound-phenanthrene))(define-sort (MUTAGENESIS-ONTOLOGY bond)	(to ! compound)	(type ! integer))