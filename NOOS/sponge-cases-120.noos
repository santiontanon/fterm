(define-episode (sponge :ID SPONGE-0)
        (EXTERNAL-FEATURES
         (DEFINE (EXTERNAL-FEATURES)
                 (BODY-SIZE SMALL)
                 (CONSISTENCY FIRM)
                 (HAND TOUGH)
                 (TOUCH ROUGH) 
                 (SURFACE EVEN)
                 (GROWING
                  (DEFINE (GROWING)
                          (LINE-FORM
                           (DEFINE (LINE-FORM)
                                   (FORM HEMISPHERICAL)
                                   (PEDUNCLE NO)))))
                 (COLOUR
                  (DEFINE (COLOUR)
                          (INTERNAL YELLOWISH)
                          (EXTERNAL (DEFINE (SET) GREY WHITISH))))
                 (PIGMENTATION NO)
                 (HOLLOW NO)
                 (BRUSH NO)
                 (MACRO-DEBRIS NONE)
                 (OSC (DEFINE (osc-characteristics) (DOT NO) (numosc one-osc) (POSOSC ELEVATED)))))
        (ECOLOGICAL-FEATURES
         (DEFINE (ECOLOGICAL-FEATURES)
                 (SUBSTRATE ROCK)
                 (LOCATION MEDITERRANEAN)))
        (SPICULATE-SKELETON
         (DEFINE (SPICULATE-SKELETON)
                         (CHEMICAL SILICA)
                         (ARCHITECTURE PERIFERY-RADIATE)
                         (MEGASCLERES
                          (DEFINE (MEGASCLERES)
                                  (SMOOTH-FORM
                                   (DEFINE (SET)
                                           (DEFINE
                                            (ORTHOTRIAENA)
                                            (MAX-DIAMETER 18))
                                           STRONGYLE))
                                  (ACANTHOSE NO-ACANTHOSE)
                                  (CHARACTERISTICS
                                   (DEFINE (CHARACTERS) (SHAFT FUSIFORM)))
                                  (ORNAMENTATION SMOOTH-ORNAMENTATION)
                                  (MAX-LENGTH 900)
                                  (TWO-OR-MORE-LENGTH NO)))
                         (MICROSCLERES
                          (DEFINE (SET)
                                  SPHERULE
                                  (DEFINE (STERRASTER)
                                          (TYPE
                                           (DEFINE (SET) SPHERICAL ELLIPTICAL))
                                          (DEFORMED NO)
                                          (STERR-DIAMETER 120))
                                  (DEFINE (OXYASTER)
                                          (DEFORMATION NO)
                                          (MAX-DIAMETER 18))))
                         (SCLEROSPONGIDS NO)
                         (UNIFORM-LENGTH NO)))
        ; (FIBROUS-SKELETON (DEFINE (NO-FIBROUS-SKELETON)))
        ; (TRACTS-SKELETON (DEFINE (NO-TRACTS-SKELETON)))
        (ANATOMY
         (DEFINE (ANATOMIC-FEATURES)
                 (ORNAMENTATION SMOOTH-ORNAMENTATION)
                 (ECTOSOME
                  (DEFINE (VISIBLE-ECTOSOME) (SEPARABLE YES) (SPICULES YES)))
                 (CORTEX
                  (DEFINE (CORTEX-CHARACTERISTICS)
                          (CORTICAL-SPICULES YES)
                          (THICK 800))))))
